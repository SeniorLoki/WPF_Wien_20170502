            <ResumeStoryboard BeginStoryboardName="myStoryBoard_BeginStoryboard"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="FrameworkElement.Loaded">
            <BeginStoryboard x:Name="myStoryBoard_BeginStoryboard" Storyboard="{StaticResource myStoryBoard}"/>
        </EventTrigger>
        <EventTrigger RoutedEvent="UIElement.MouseLeave">
            <PauseStoryboard BeginStoryboardName="myStoryBoard_BeginStoryboard"/>
        </EventTrigger>
    </Window.Triggers>
